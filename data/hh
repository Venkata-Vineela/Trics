import json

# Read data from the file and extract information
with open('healthcareLocations.json', 'r') as file:
    data = json.load(file)

# Extracting the desired information
extracted_data = []

for record in data['records']:
    _id, name, address, city, state, zip_code, phone, longitude, latitude, location = record
    extracted_data.append({
        'latitude': float(latitude),
        'longitude': float(longitude),
        'title': name
    })

# Printing the extracted data
print(extracted_data)
